{
  "name": "braintree-web-drop-in",
  "version": "1.19.0",
  "main": "src/index.js",
  "private": true,
  "scripts": {
    "build": "gulp build",
    "development": "gulp development",
    "server": "gulp server",
    "build:gh-pages": "gulp build:gh-pages",
    "lint": "eslint src test",
    "sauceconnect": "node scripts/sauce-connect.js",
    "hint": "hint http://localhost:4567",
    "pretest": "npm run lint",
    "test": "karma start test/config/karma.js --single-run",
    "test:hint": "npm-run-all -n -s build -pr server hint",
    "test:integration": "bundle exec rake test",
    "test:integration:paypal-only": "RUN_PAYPAL_ONLY=true npm run test:integration",
    "test:integration:paypal-skipped": "SKIP_PAYPAL=true npm run test:integration",
    "test:integration:kill": "ps -ef | awk '/rspec/ {print $2}' | xargs kill",
    "test:publishing": "mocha test/publishing",
    "deploy:gh-pages": "./scripts/deploy-gh-pages"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/braintree/braintree-web-dropin.git"
  },
  "license": "MIT",
  "devDependencies": {
    "ansi-colors": "^4.1.0",
    "brfs": "^2.0.2",
    "browserify": "^16.3.0",
    "browserify-versionify": "^1.0.6",
    "chai": "^4.2.0",
    "connect": "^3.7.0",
    "del": "^5.0.0",
    "dotenv": "^8.0.0",
    "eslint": "^5.16.0",
    "eslint-config-braintree": "^3.0.1",
    "fancy-log": "^1.3.3",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.2.0",
    "gulp-less": "^4.0.1",
    "gulp-rename": "^1.4.0",
    "gulp-replace": "^1.0.0",
    "gulp-size": "3.0.0",
    "gulp-streamify": "1.0.2",
    "gulp-uglify": "^3.0.2",
    "hint": "^5.1.1",
    "is-uuid": "1.0.2",
    "jsdoc": "^3.6.3",
    "jsdoc-template": "braintree/jsdoc-template#3.2.0",
    "karma": "^4.2.0",
    "karma-browserify": "^6.1.0",
    "karma-mocha": "1.3.0",
    "karma-mocha-reporter": "2.2.5",
    "karma-phantomjs-launcher": "1.0.4",
    "mkdirp": "0.5.1",
    "mocha": "^6.2.0",
    "npm-run-all": "^4.1.5",
    "phantomjs-prebuilt": "2.1.16",
    "sauce-connect-launcher": "^1.2.7",
    "serve-static": "^1.14.1",
    "sinon": "^7.3.2",
    "sinon-chai": "^3.3.0",
    "through2": "^3.0.1",
    "vinyl": "^2.2.0",
    "vinyl-source-stream": "^2.0.0",
    "watchify": "^3.11.1",
    "webhint-configuration-braintree-sdk": "github:braintree/webhint-configuration-braintree-sdk#semver:0.0.4"
  },
  "dependencies": {
    "@braintree/asset-loader": "0.3.1",
    "@braintree/browser-detection": "1.7.0",
    "@braintree/class-list": "0.1.0",
    "@braintree/event-emitter": "0.2.0",
    "@braintree/wrap-promise": "2.0.0",
    "braintree-web": "3.49.0",
    "promise-polyfill": "8.1.3"
  },
  "browserify": {
    "transform": [
      "brfs",
      "browserify-versionify"
    ]
  },
  "browserslist": [
    "last 2 versions",
    "iOS 8"
  ]
}
